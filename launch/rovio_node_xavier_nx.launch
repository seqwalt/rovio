<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <node pkg="rovio" type="rovio_node" name="rovio" output="screen">
  <param name="filter_config" value="$(find rovio)/cfg/xavier_nx/rovio_test.info"/>
  <param name="camera0_config" value="$(find rovio)/cfg/xavier_nx/rovio_cam0.yaml"/>
  <param name="camera1_config" value="$(find rovio)/cfg/xavier_nx/rovio_cam1.yaml"/>
  <remap from="/cam0/image_raw" to="/camera/fisheye1/image_raw" />
  <remap from="/cam1/image_raw" to="/camera/fisheye2/image_raw" />
  <remap from="/imu0" to="/hummingbird/vi_sensor/imu" />
  </node>
</launch>
